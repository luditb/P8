    "def url_to_image(url):\n",
    "    resp = urllib.request.urlopen(url)\n",
    "    img_bytes = Image.open(io.BytesIO(resp.read())).resize((224, 224)) #224 is the size of transfer learning methods\n",
    "    return img_bytes"
   ]
  },
